{
  "expo": {
    "name": "Quicklist",
    "slug": "Quicklist",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "quicklist",
    "description": "Organize your life, effortlessly<",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/images/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "dev.frerot.quicklist"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        { "resizeMode": "contain", "backgroundColor": "#ffffff" }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "owner": "frerot",
    "privacy": "public"
  }
}
